{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "full_name": {
      "type": "string"
    },
    "department": {
      "type": "string"
    },
    "phone_number": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "create_at": {
      "type": "string",
      "format": "date-time"
    },
    "update_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "id",
    "email",
    "full_name",
    "department",
    "phone_number",
    "create_at",
    "update_at"
  ],
  "additionalProperties": false
}
